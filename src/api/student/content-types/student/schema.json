{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": false,
      "unique": true
    },
    "phone": {
      "type": "string"
    },
    "dob": {
      "type": "string"
    },
    "school_name": {
      "type": "string"
    },
    "grade": {
      "type": "string"
    },
    "section": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "mail_sent": {
      "type": "boolean"
    },
    "wa_sent": {
      "type": "boolean"
    },
    "payment_id": {
      "type": "string"
    },
    "school": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::school.school"
    },
    "is_overseas": {
      "type": "boolean"
    },
    "payment_status": {
      "type": "enumeration",
      "default": "pending",
      "enum": ["pending", "completed", "failed"]
    },
    "selected_addon": {
      "type": "json"
    },
    "order_amount": {
      "type": "biginteger"
    },
    "order_currency": {
      "type": "string"
    },
    "razorpay_order_id": {
      "type": "string"
    },
    "payment_verified_at": {
      "type": "datetime"
    },
    "payment_method": {
      "type": "string"
    },
    "payment_captured_at": {
      "type": "datetime"
    }
  }
}
